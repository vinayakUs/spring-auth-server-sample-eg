[ req ]
default_bits       = 2048
prompt             = no
default_md         = sha256
distinguished_name = dn
req_extensions     = req_ext

[ dn ]
C  = US
O  = Spring
OU = Samples
CN = demo-client-sample

[ req_ext ]
subjectAltName = @alt_names

[ alt_names ]
DNS.1 = client
DNS.2 = localhost
IP.1  = 127.0.0.1
